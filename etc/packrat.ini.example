[packrat]
host = 0.0.0.0
port = 5000
# Whether or not to turn on debugging.
debug = True

# If storage location does not start with / it will be relative
# to the packrat root.
storage_location = storage

# Max total size of everything stored in the cache.
# Temporary files may go over this value, so set it conservatively.
cache_size = 104857600 # 100 MB